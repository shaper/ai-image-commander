{
  "name": "ai-image-commander",
  "version": "0.0.1",
  "description": "A CLI tool for generating images using AI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aic": "./dist/cli.js"
  },
  "type": "module",
  "files": ["dist", "public"],
  "scripts": {
    "build": "tsup && copyfiles \"aic.conf.example\" dist && copyfiles -u 1 \"public/**/*\" dist/public",
    "dev": "tsup --watch",
    "lint": "biome check",
    "lint:fix": "biome check --write --unsafe",
    "format": "biome format",
    "format:fix": "biome format --write",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "@ai-sdk/deepinfra": "^0.1.9",
    "@ai-sdk/fal": "^0.0.2",
    "@ai-sdk/fireworks": "^0.1.8",
    "@ai-sdk/google-vertex": "^2.1.12",
    "@ai-sdk/luma": "^0.0.4",
    "@ai-sdk/openai": "^1.1.9",
    "@ai-sdk/provider": "^1.0.7",
    "@ai-sdk/replicate": "^0.1.7",
    "@ai-sdk/togetherai": "^0.1.9",
    "@inquirer/core": "^10.1.6",
    "@inquirer/prompts": "^7.3.1",
    "ai": "latest",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "image-type": "^5.2.0",
    "open": "^10.1.0",
    "ora": "^8.2.0",
    "sharp": "^0.33.5",
    "terminal-image": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^20.17.17",
    "copyfiles": "^2.4.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
