{
  "name": "ai-image-generator-cli",
  "version": "0.0.1",
  "description": "A CLI tool for generating images using AI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aic": "./dist/cli.js"
  },
  "type": "module",
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "build": "tsup && copyfiles -u 1 \"public/**/*\" dist/public",
    "dev": "tsup --watch",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "latest",
    "ai": "latest",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "open": "^10.1.0",
    "prettier": "^3.4.2",
    "terminal-image": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "copyfiles": "^2.4.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
