{
  "name": "ai-image-commander",
  "version": "0.0.2",
  "description": "A CLI tool for generating images using AI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aic": "./dist/cli.js"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && copyfiles \"aic.conf.example\" dist",
    "dev": "tsup --watch",
    "lint": "biome check",
    "lint:fix": "biome check --write --unsafe",
    "format": "biome format",
    "format:fix": "biome format --write",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "@ai-sdk/deepinfra": "^0.2.10",
    "@ai-sdk/fal": "^0.1.7",
    "@ai-sdk/fireworks": "^0.2.9",
    "@ai-sdk/google-vertex": "^2.2.14",
    "@ai-sdk/luma": "^0.1.6",
    "@ai-sdk/openai": "^1.3.10",
    "@ai-sdk/provider": "^1.1.2",
    "@ai-sdk/replicate": "^0.2.6",
    "@ai-sdk/togetherai": "^0.2.9",
    "@changesets/cli": "^2.29.2",
    "@inquirer/core": "^10.1.10",
    "@inquirer/prompts": "^7.4.1",
    "ai": "^4.3.5",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "image-type": "^5.2.0",
    "open": "^10.1.0",
    "ora": "^8.2.0",
    "sharp": "^0.34.1",
    "terminal-image": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.14.1",
    "copyfiles": "^2.4.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
